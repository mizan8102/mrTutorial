<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tutorials_utc">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Account_Activity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.tutorials_utc.Home" />
        </activity>
        <activity android:name=".About_Activity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.tutorials_utc.Home" />
        </activity>
        <activity android:name=".Tutorial_viewActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.tutorials_utc.Home" />
        </activity>
        <activity android:name=".Tutorial_Activity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.tutorials_utc.Home" />
        </activity>
        <activity android:name=".Home" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>